{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "ESNext",
    "target": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "allowJs": true,
    "noImplicitAny": true,
    "removeComments": true,
    "allowUnreachableCode": false,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src"]
}
